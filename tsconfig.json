{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "baseUrl": ".",
    "rootDir": ".",
    "strict": true,
    "skipLibCheck": false,
    "noImplicitOverride": true,
    "noEmit": true,
    "esModuleInterop": true,
  },
  "include": [
    "common/**/*",
    "client/**/*.d.mts",
    "util.d.mts",
    "client/canvas/groups/canvas-group-mixin.mts",
    "client/applications/sheets/token/mixin.mts",
    "client/packages/client-package.mts"
  ]
}
